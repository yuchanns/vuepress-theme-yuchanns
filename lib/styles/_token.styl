div[class*=language-]
  position relative
  background-color #f6f8fa
  border-radius 6px
  pre
  pre[class*=language-]
    background transparent
    position relative
    z-index 1
  .highlight-lines
    user-select none
    padding-top 1.0rem
    position absolute
    top 0
    left 0
    width 100%
    line-height 1.2
    .highlighted
      background-color #fffbdd

.token
  &.keyword
  &.operator
    color #d73a49
  &.function-variable
    color #6f42c1
  &.function
    color #005cc5
  &.parameter
    color #24292e
  &.number
  &.constant
  &.boolean
    color #005cc5
  &.double-quoted-string
  &.single-quoted-string
  &.string
    color #032f62
  &.variable
    color #24292e
  &.comment
    color #6a737d
  &.builtin
    color #e36209
  &.class-name
    color #6f42c1